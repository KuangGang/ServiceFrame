<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.choice.scm.dao.mapper.DemoDao">

    <insert id="insert" parameterType="com.choice.scm.entity.scm.Demo">
      INSERT INTO demo (id, test) values (#{id},#{test})
    </insert>

    <select id="findAll" resultType="com.choice.scm.entity.scm.Demo">
        SELECT * from demo
    </select>
</mapper>